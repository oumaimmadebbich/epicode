<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ChoseResetController">
   <children>
      <Label layoutX="38.0" layoutY="74.0" prefHeight="30.0" prefWidth="407.0" text="Do you want to reset your password using email or phone number?" />
      <TextField fx:id="emailReset" layoutX="60.0" layoutY="164.0" prefHeight="30.0" prefWidth="188.0" />
      <TextField fx:id="phoneReset" layoutX="60.0" layoutY="238.0" prefHeight="30.0" prefWidth="188.0" />
      <Button fx:id="emailBoutton" layoutX="307.0" layoutY="164.0" mnemonicParsing="false" onAction="#btSendEmail" prefHeight="25.0" prefWidth="88.0" text="Email" />
      <Button fx:id="phoneBoutton" layoutX="307.0" layoutY="238.0" mnemonicParsing="false" onAction="#btSendPhone" prefHeight="25.0" prefWidth="88.0" text="Telephone" />
   </children>
</AnchorPane>
